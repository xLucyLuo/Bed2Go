<!DOCTYPE html>
<html>
  <head>
    <title>Mybnb</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['MAPS_API_KEY']}" %>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Oxygen:wght@300;400;700&family=Quicksand:wght@300;400;700&family=Red+Hat+Display:wght@300;400;700&family=Roboto:wght@300;700&family=Roboto:wght@300;700&display=swap" rel="stylesheet">

  </head>

  <body>
    <%= yield %>
  </body>
</html>
